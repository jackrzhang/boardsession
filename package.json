    "eslint-plugin-react": "^6.4.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "morgan": "^1.8.1",
    "postcss-assets": "^4.1.0",
    "postcss-import": "^8.1.2",
